    "postinstall-postinstall": "^2.1.0",
    "shelljs": "^0.8.5",
    "simple-git": "^3.18.0",
    "template-file": "^6.0.1",
    "vite-node": "^0.29.8",
    "yargs": "^17.7.1",
    "zod": "^3.20.6"
