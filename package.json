  "keywords": [
    "cli",
    "typescript"
  ],
  "scripts": {
    "test": "vitest run",
    "start": "npm run start:dev",
    "start:dev": "tsup main.ts --watch --format cjs,esm --dts --onSuccess=\"NODE_ENV=development node ./dist/main.js\"",
    "start:dev:name": "tsup main.ts --watch --format cjs,esm --dts --onSuccess=\"NODE_ENV=development node ./dist/main.js --name LoginScreen\"",
    "start:dev:template": "tsup main.ts --watch --format cjs,esm --dts --onSuccess=\"NODE_ENV=development node ./dist/main.js --template screen\"",
    "start:dev:template:name": "tsup main.ts --watch --format cjs,esm --dts --onSuccess=\"NODE_ENV=development node ./dist/main.js --name LoginScreen --template screen\"",
    "postinstall": "patch-package",
    "lint": "tsc",
    "build": "tsup main.ts --dts --format cjs,esm",
    "ci": "pnpm run lint && pnpm run test && pnpm run build",
    "release": "pnpm run lint && pnpm run test && pnpm run build && changeset publish"
  },
  "dependencies": {
    "@effect-ts/core": "^0.60.5",
