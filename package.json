{
  "name": "@kieran-osgood/scribe",
  "version": "0.2.0",
  "description": "",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "scribe": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kieran-osgood/scribe.git"
  },
  "homepage": "https://github.com/kieran-osgood/scribe/#readme",
  "bugs": "https://github.com/kieran-osgood/scribe/issues",
  "license": "MIT",
  "keywords": [
    "cli",
    "typescript"
  ],
  "scripts": {
    "start": "ts-node ./index.ts",
    "test": "pnpm run build && chmod +x ./dist/index.js && vitest",
    "lint": "eslint src --fix --ext .js,.ts,.tsx",
    "typecheck": "tsc",
    "build": "tsup index.ts",
    "ci": "run-p ci:*",
    "ci:typecheck": "pnpm run typecheck",
    "ci:lint": "pnpm run lint",
    "ci:test": "pnpm run test",
    "ci:build": "pnpm run build",
    "release": "run-s release:*",
    "release:ci": "pnpm run ci",
    "release:publish": "changeset publish",
    "changeset": "changeset",
    "postinstall": "patch-package",
    "size": "pnpm run build && size-limit"
  },
  "dependencies": {
    "@effect/data": "^0.12.5",
    "@effect/io": "^0.26.0",
    "@effect/match": "^0.24.0",
    "@effect/schema": "^0.20.0",
    "@effect/stream": "^0.22.0",
    "case-anything": "^2.1.10",
    "clipanion": "^3.2.0",
    "colorette": "^2.0.20",
    "cosmiconfig": "^8.1.0",
    "cosmiconfig-typescript-loader": "^4.3.0",
    "fp-ts": "^2.13.1",
    "fp-ts-std": "^0.15.1",
    "inquirer": "8.0.0",
    "simple-git": "^3.18.0",
    "strip-ansi": "^7.1.0",
    "template-file": "^6.0.1",
    "typanion": "^3.12.1",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@expo/spawn-async": "^1.7.2",
    "@gmrchk/cli-testing-library": "^0.1.2",
    "@size-limit/file": "^8.2.4",
    "@swc/core": "^1.3.57",
    "@swc/helpers": "^0.5.1",
    "@total-typescript/ts-reset": "^0.3.7",
    "@tsconfig/node18": "^1.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.14.0",
    "@types/yargs": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@vitest/coverage-istanbul": "^0.31.1",
    "changeset": "^0.2.6",
    "eslint": "^8.34.0",
    "eslint-plugin-communist-spelling": "^1.0.0",
    "fast-check": "^3.8.0",
    "get-stream": "^7.0.0",
    "memfs": "^3.5.1",
    "memorystream": "^0.3.1",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.5.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.8.4",
    "shx": "^0.3.4",
    "size-limit": "^8.2.4",
    "tempy": "^3.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "unionfs": "^4.4.0",
    "vite-node": "^0.29.8",
    "vitest": "0.31.1"
  },
  "size-limit": [
    {
      "path": "dist/main.js",
      "limit": "10 kb"
    }
  ],
  "pnpm": {
    "patchedDependencies": {
      "clipanion@3.2.0": "patches/clipanion@3.2.0.patch"
    }
  }
}
