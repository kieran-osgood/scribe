  "description": "",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "scribe": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/@kieran-osgood/scribe.git"
